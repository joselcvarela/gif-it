{
  "manifest_version": 2,

  "name": "Gif It",
  "description": "Simple search for GIFs",
  "author": "Jos√© Varela (joselcvarela@gmail.com)",
  "version": "1.1",

  "browser_action": {
   "default_icon": "icon32.png",
   "default_popup": "popup.html"
  },
  "icons": {
    "16": "icon16.png",
    "32": "icon32.png",
    "128": "icon128.png"
  },
  "permissions": [
    "storage",
    "activeTab",
    "*://*.giphy.com/*"
   ]
}
